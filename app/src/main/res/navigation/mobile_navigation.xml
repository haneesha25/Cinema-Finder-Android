<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.cinemafinderandroid.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.cinemafinderandroid.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.cinemafinderandroid.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />
    <activity
        android:id="@+id/userProfileActivity2"
        android:name="com.example.cinemafinderandroid.UserProfileActivity"
        android:label="activity_user_profile"
        tools:layout="@layout/activity_user_profile" />
    <activity
        android:id="@+id/addTheaterActivity2"
        android:name="com.example.cinemafinderandroid.AddTheaterActivity"
        android:label="activity_add_theater"
        tools:layout="@layout/activity_add_theater" />
    <activity
        android:id="@+id/addMovieActivity"
        android:name="com.example.cinemafinderandroid.AddMovieActivity"
        android:label="activity_add_movie"
        tools:layout="@layout/activity_add_movie" />
    <activity
        android:id="@+id/viewTheaterActivity"
        android:name="com.example.cinemafinderandroid.ViewTheatersActivity"
        android:label="activity_view_theaters"
        tools:layout="@layout/activity_view_theaters" />
    <activity
        android:id="@+id/viewMovieActivity"
        android:name="com.example.cinemafinderandroid.ViewMovieActivity"
        android:label="activity_view_movie"
        tools:layout="@layout/activity_view_movie" />


</navigation>